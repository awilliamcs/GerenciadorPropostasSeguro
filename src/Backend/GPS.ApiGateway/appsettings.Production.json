{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "GPS.ApiGateway.Middleware.RequestLoggingMiddleware": "Information",
      "Ocelot": "Information"
    }
  },
  "AllowedHosts": "*",
  "Ocelot": {
    "RateLimitOptions": {
      "EnableRateLimiting": true,
      "Period": "1m",
      "PeriodTimespan": 60,
      "Limit": 200
    }
  }
}
